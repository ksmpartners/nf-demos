plugins {
  id 'nextfuse@0.11.1'
}

process {
  executor = 'domino'
  withLabel:process_low {
    ext.hardwareTierName = 'Mini'
  }
}

domino {
  hardwareTierId = 'small-k8s'
  environmentName = 'biocontainers/sra-tools:3.0.8'
  imageMap = [
    'quay.io/biocontainers/sra-tools:2.11.0--pl5321ha49a11a_3':'biocontainers/sra-tools:2.11.0--pl5321ha49a11a_3',
    'quay.io/biocontainers/python:3.9--1':'biocontainers/sra-tools:3.0.8']
}
